{
  "builds": [
    {
      "src": "app/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "app/server.py"
    },
    {
      "src": "/(.*)",
      "dest": "app/wsgi.py"
    }
  ]
}